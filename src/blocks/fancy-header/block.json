{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "title": "Educast-Header",
  "name": "educast/educast-header",
  "category": "text",
  "icon": "heading",
  "description": "Adds a header with a hover effect",
  "keywords": [
    "header",
    "underline",
    "hover"
  ],
  "version": "1",
  "textdomain": "educast-blocks",
  "editorScript": "file:./index.js",
  "supports": {
    "spacing": {
      "margin": true
    }
  },
  "style": "file:./index.css",
  "attributes": {
    "title": {
      "type": "string",
      "source": "html",
      "selector": ".fancy-header"
    },
    "underline_color": {
      "type": "string",
      "default": "#f87171"
    }
  }
}
